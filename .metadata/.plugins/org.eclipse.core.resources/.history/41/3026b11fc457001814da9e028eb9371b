package algorithms;

public class SortingAlgos
{
	public void insertionSort(int[] nums)
	{
		int n = nums.length;
		if (n <= 1)
			return;

		for (int i = 1; i < n; i++)
		{
			int curr = nums[i];
			int j = i - 1;
			while (j > 0)
			{
				if (nums[j] > curr)
				{
					nums[j + 1] = nums[j];
					j--;
				}
				else
				{
					nums[j] = curr;
					break;
				}
			}
		}
	}
}
