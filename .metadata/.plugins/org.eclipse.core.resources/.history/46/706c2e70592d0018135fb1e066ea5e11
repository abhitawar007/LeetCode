package algorithms;

public class DynamicProgramming
{
	public int fibonacci(int n)
	{
		// Recursive working
		// if (n == 0 || n == 1)
		// return n;
		// return fibonacci(n - 1) + fibonacci(n - 2);

		// _________DP____________
		int[] arr = new int[n + 1];
		
		arr[0] = 0;
		arr[1] = 1;
		for(int i=2; i<n; i++)
			arr[i] = arr[i-1] + arr[i-2];
		
		return arr[n-1] + arr[n-2];
	}
}
