package oop;

public class MainClass
{
	public static void main(String[] args)
	{
		CallByConcepts c1 = new CallByConcepts(1, 2);
		CallByConcepts c2 = new CallByConcepts(5, 6);

	}

	public void swap(CallByConcepts arg1, CallByConcepts arg2)
	{
		arg1.a = 11;
		arg1.b = 12;

		CallByConcepts temp = arg1;
		arg1 = arg2;
		arg2 = temp;
	}
}
