import java.util.HashMap;
import java.util.Hashtable;

public class mainClass
{

	public static void main(String[] args)
	{
		HashMap<Integer, Character> map = new HashMap<>();
		Hashtable<Integer, Character> table = new Hashtable<>();

		map.put(1, 'A');
		map.put(1, 'B');

		table.put(1, 'C');
		table.put(1, 'D');
	}

}
