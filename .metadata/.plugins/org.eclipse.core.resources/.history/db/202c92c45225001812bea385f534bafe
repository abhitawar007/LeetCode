package testing;

import algorithms.TreeAlgos;
import algorithms.TreeNode;

public class TreeTesting
{

	public static void main(String[] args)
	{
		TreeAlgos tree = new TreeAlgos();
		// int[] arr = { 1, 2, 3, 4, 5 };
		int[] arr = { -10, -5, 0, 5, 10 };

		TreeNode root = tree.createBSTfromArray(arr);
		tree.printTree(root);
	}

}
